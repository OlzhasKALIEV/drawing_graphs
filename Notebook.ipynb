import requests
from IPython.display import Image, display
# Получить список картинок из сервера
response = requests.get('http://localhost:5000/api/v1/drawing_graphs/all/')
data = response.json()
images = data['images']
# Отобразить картинку
for image in images:
    image_url = image['url']
    display(Image(url=image_url))
